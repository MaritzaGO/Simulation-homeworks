<p id="notice"><%= notice %></p>

<h1>Listado de Respuesta a las tareas</h1>

<table>
  <thead>
    <tr>
      <th>Message</th>
      <th>Respuesta a Tarea</th>
      <th>Calificacion</th>
      <th>Fecha</th>
      <th>Hour</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @respuesta.each do |respuestum| %>
      <tr>
        <td><%= respuestum.message %></td>
        <td><%= respuestum.calificacion %></td>
        <td><%= respuestum.respuestatarea %></td>
        <td><%= respuestum.fecha %></td>
        <td><%= respuestum.hour %></td>
        <td><%= link_to 'Show', respuestum %></td>
        <td><%= link_to 'Edit', edit_respuestum_path(respuestum) %></td>
        <td><%= link_to 'Destroy', respuestum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Respuestum', new_respuestum_path %>
