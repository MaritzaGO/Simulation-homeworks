<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @estudiante.name %>
</p>

<p>
  <strong>Lastname:</strong>
  <%= @estudiante.lastname %>
</p>

<p>
  <strong>Identificacion:</strong>
  <%= @estudiante.identificacion %>
</p>

<p>
  <strong>Asignatura(s):</strong>
  <ul>
  <% @estudiante.asignaturas.each do |a| %>
    <li><%= a.name %></li>
  <% end %>
  </ul>
</p>

<p>
  <strong>Email:</strong>
  <%= @estudiante.email %>
</p>

<p>
  <strong>Edad:</strong>
  <%= @estudiante.edad %>
</p>

<p>
  <strong>Gender:</strong>
  <%= @estudiante.gender %>
</p>

<%= link_to 'Edit', edit_estudiante_path(@estudiante) %> |
<%= link_to 'Back', estudiantes_path %>
